@{
    ViewData["Title"] = "Adicionar entrada de veículo";
}
<h1>@ViewData["Title"]</h1>


<form>
    <div class="row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">Nome do Cliente Responsável:</label>
            <input type="email" class="form-control" id="inputEmail4" placeholder="Nome">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">CPF:</label>
            <input type="password" class="form-control" id="inputPassword4" placeholder="Senha">
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">Nome do cliente (opcional):</label>
            <input type="email" class="form-control" id="inputEmail4" placeholder="Nome">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">CPF:</label>
            <input type="password" class="form-control" id="inputPassword4" placeholder="Senha">
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            <label for="ddlTipoVeiculo">Tipo de veículo:</label>
            <select id="ddlTipoVeiculo" class="form-select" aria-label="Default select example">
                <option selected>Open this select menu</option>
                <option value="1">One</option>
                <option value="2">Two</option>
                <option value="3">Three</option>
            </select>
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">Modelo:</label>
            <input type="email" class="form-control" id="inputEmail4" placeholder="Nome">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">Placa:</label>
            <input type="password" class="form-control" id="inputPassword4" placeholder="Senha">
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            <div class="cs-form">
                <label for="inputPassword4">Hora da Entrada:</label>
                <input type="time" class="form-control" value="10:05 AM" />
            </div>
        </div>
        <div class="form-group col-md-6">
            <div class="cs-form">
                <label for="inputPassword4">Local:</label>
                <input type="email" class="form-control" id="inputEmail4" placeholder="Nome">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-6">
            <div class="cs-form">
                <button class="btn btn-primary">Salvar</button>
            </div>
        </div>
    </div>

    
